{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@assets": "./src/assets",
          "@comp": "./src/components",
          "@components": "./src/components",
          "@config": "./src/config",
          "@containers": "./src/containers",
          "@hooks": "./src/hooks",
          "@lib": "./src/lib",
          "@utils": "./src/utils",
          "assets": "./src/assets",
          "comp": "./src/components",
          "components": "./src/components",
          "config": "./src/config",
          "containers": "./src/containers",
          "hooks": "./src/hooks",
          "lib": "./src/lib",
          "utils": "./src/utils"
        },
        "extensions": [".js", ".jsx", ".es", ".es6", ".mjs"]
      }
    ]
  ]
}
